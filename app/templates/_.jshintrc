{
<% if (props.jsPreprocessor.srcExtension === 'es6') { -%>
  "esnext": true,
<% } else { -%>
  "strict": true,
<% } -%>
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "latedef": false,
  "noarg": true,
  "undef": true,
  "unused": true,
  "validthis": true,
  "jasmine": true,
  "globals": {
    "angular": false,
    "inject": false,
    "module": false
  }
}
